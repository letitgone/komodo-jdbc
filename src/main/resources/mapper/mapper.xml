<mapper namespace="User">

    <select id="selectOne" parameterType="com.komodo.jdbc.entity.User"
            resultType="com.komodo.jdbc.entity.User">
        select * from user where id = {id} and username = #{username}
    </select>

    <select id="selectList" resultType="com.komodo.jdbc.entity.User">
        select * from user
    </select>

</mapper>
